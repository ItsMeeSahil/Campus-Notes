    <title>Coding Playlist</title>
    <style>
        *{
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }
        html,body{
            height: 100%;
            width: 100%;
        }
        .heading{
            font-size:50px;
            text-align: center;
            margin: 30px;
            animation: fadeIn s ease-in-out;
        }
        .main{
            padding: 10px;
            background-color: #444444;
            margin: 15px;
            height: 1000px;
            display: flex;
            justify-content: center;
            align-items: center;
            border: 1px solid black;
            border-radius: 15px;
        }
        .p1,.p2{
            padding: 10px;
            margin: 15px;
            height: 100%;
            display: flex;
            flex-direction: column;
            justify-content: center;
            align-items: center;
            cursor: pointer;
        }
        a{
            color: black;
        }
        .Playlist {
            display: flex;
            align-items: center;
            overflow: hidden;
            background-color: #f8f8f8;
            border: 1px solid grey;
            border-radius: 10px;
            box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
            overflow: hidden;
            transition: transform 0.3s ease, box-shadow 0.3s ease;
            height: 50%;
            width: 80%;
            margin-top: 10px;
            animation: slideInUp 1s ease forwards;
        }
    .Playlist:hover {
        transform: scale(1.02);
        box-shadow: 0 8px 12px rgba(255, 255, 255, 0.2);
    }
    .Playlist img{
        height: 100%;
        border-right: 2px solid black;
    
    }
    .Playlist a{
        padding-left: 50px;
        text-decoration: none;
    }
    @keyframes fadeIn {
        from { opacity: 0; transform: translateY(-20px); }
        to { opacity: 1; transform: translateY(0); }
    }

    @keyframes slideInUp {
        from { opacity: 0; transform: translateY(50px); }
        to { opacity: 1; transform: translateY(0); }
    }
    .footer {
        height: 100px;
        width: 100%;
        background-color: #ffffff;
        color: #000000;
        display: flex;
        align-items: center;
        justify-content: center;
        text-align: center;
        padding: 20px;
    }
    </style>
